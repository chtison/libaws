{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
		"outDir": "dist/",
		"listEmittedFiles": true,
        "sourceMap": false,
		"forceConsistentCasingInFileNames": true,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strictNullChecks": true,
		"lib": [
			"es2015",
			"es2015.promise"
		]
    }
}
